{
  "expo": {
    "name": "QAssist",
    "slug": "qassist-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/qassist-icon.png",
    "scheme": "qassistapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/qassist-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "splash": {
        "image": "./assets/images/qassist-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "infoPlist": {
        "NSCameraUsageDescription": "Bu uygulama görev kanıt fotoğrafları çekmek için kameraya erişim gerektirir.",
        "NSPhotoLibraryUsageDescription": "Bu uygulama görev kanıt fotoğrafları seçmek için fotoğraf galerisine erişim gerektirir."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/qassist-icon.png"
      },
      "splash": {
        "image": "./assets/images/qassist-icon.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "package": "com.digivisor.qassistapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/qassist-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/qassist-icon.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d7fa03b9-0f53-41f6-8d6a-9dc6f1fa4b7d"
      }
    }
  }
}
